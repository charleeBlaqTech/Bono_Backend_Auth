<main id="profile-main">
    <section class="profile-section section-pd">
        {{!-- <div class="user-profile-image-wrapper">
            <img src="" alt="">
        </div> --}}
        <div class="user-profile-details">
            <div class="user-names">
                <p><span>Firstname:</span><span>{{currentUserDetail.firstname}}</span></p>
                <p><span>Lastname:</span><span>{{currentUserDetail.lastname}}</span></p>
            </div>
            <div class="user-address">
                
                <p><span>City:</span><span>{{currentUserDetail.city}}</span></p>
                <p><span>Nationality:</span><span>{{currentUserDetail.country}}</span></p>
                
            </div>
            <div class="user-contact">
                <p><span>Email:</span><span>{{currentUserDetail.email}}</span></p>
            </div>
            <div class="user-trade-type">
                <p><span>Trade-Type:</span><span>{{currentUserDetail.tradeType}}</span></p>
            </div>
            <p><a href="/password/new">Do you want to change your password? click</a></p>
            
        </div>  
    </section>
</main>